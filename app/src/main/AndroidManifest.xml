<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="net.FitnessDuo.combination2">

    <uses-feature
        android:name="android.hardware.camera"
        android:required="true" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo2_background"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="net.FitnessDuo.combination2.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="net.FitnessDuo.combination2.NewPhotoActivity"
            android:windowSoftInputMode="stateVisible" />
        <activity
            android:name="net.FitnessDuo.combination2.progressPage"
            android:parentActivityName="net.FitnessDuo.combination2.MainActivity">

            <!-- The meta-data tag is required if you support API level 15 and lower -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="net.FitnessDuo.combination2.MainActivity" />
        </activity>
        <activity android:name="net.FitnessDuo.combination2.EditEntryActivity" />
        <activity
            android:name="net.FitnessDuo.combination2.Compare"
            android:parentActivityName="net.FitnessDuo.combination2.MainActivity">

            <!-- The meta-data tag is required if you support API level 15 and lower -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="net.FitnessDuo.combination2.MainActivity" />
        </activity>
        <activity
            android:name="net.FitnessDuo.combination2.GraphActivity"
            android:parentActivityName="net.FitnessDuo.combination2.MainActivity">

            <!-- The meta-data tag is required if you support API level 15 and lower -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="net.FitnessDuo.combination2.MainActivity" />
        </activity>
        <activity
            android:name="net.FitnessDuo.combination2.ImportExport"
            android:parentActivityName="net.FitnessDuo.combination2.MainActivity">

            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="net.FitnessDuo.combination2.MainActivity" />
        </activity>
    </application>

</manifest>